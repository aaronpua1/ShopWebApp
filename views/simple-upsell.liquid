  {% if product.metafields.suop != blank %}
    {% assign metafields = product.metafields.suop %}
  {% assign configs = metafields.su1 %}
    {% if configs contains "status:on" %}
      <div id="simple-upsell" class="bootstrap">
        <input type="hidden" id="configs" name="configs" value="{{ configs }}" />
        <div class="container upsell-container" style="margin-top:20px;">
          {% assign upsell_values = metafields.su1 | split: ";"%}
          {% for upsell_value in upsell_values %}
            {% if upsell_value contains "offer_title:" %}
              <h3>{{ upsell_value | replace: "offer_title:", ""}}</h3>
            {% endif %}
            {% if upsell_value contains "offer_description:" %}
              <h5>{{ upsell_value | replace: "offer_description:", ""}}</h5>
              {% break %}
            {% endif %}
          {% endfor %}
          <form id="add-upsell-form" onsubmit="submitUpsell(); return false;">
            <div class="row upsell-row">
              {% assign variants = product.variants %}
              {% assign variantSize = product.variants | size %}
              <div class="col-xs-10 col-sm-4 col-md-1 col-lg-1 upsell-product">
                <div class="col-item" id="item1" value="{{  }}">
                  <span class="tooltiptext">{{ product.title }}</span>
                  <div class="photo"><img alt="a" class="img-responsive upsell-image" src="{{ product | img_url: 'medium' }}"></div>
                  <div class="info">
                    <div class="row">
                      <div class="price col-md-12">
                        <h5>{{ product.title }}</h5>
                        <h5 id="variant-price1" class="price-text-color">{{ variants[0].price | money }}</h5>
                      </div>
                      <div class="variant col-md-12">
                        {% if variantSize <= 1 %}
                        <select id="variant1" class="variant-select" name="variant1" >
                          {% for variant in variants %}
                          <option value="{{ variant.id }}-{{ variant.price | money }}">{{ variant.title }}</option>
                          {% endfor %}
                        </select>
                        {% else %}
                        <select id="variant1" class="variant-select" name="variant1">
                          {% for variant in variants %}
                          <option value="{{ variant.id }}-{{ variant.price | money }}">{{ variant.title }}</option>
                          {% endfor %}
                        </select>
                        {% endif %}
                      </div>
                    </div>
                    <div class="rating col-md-12">
                      <i class="price-text-color fa fa-star"></i><i class="price-text-color fa fa-star"></i><i class="price-text-color fa fa-star"></i><i class="price-text-color fa fa-star"></i><i class="fa fa-star"></i>
                    </div>
                    <div class="separator">
                      <p id="btn-id1" class="btn-add"><input type="checkbox" id="c1" name="cb[]" value="{{ variants[0].id }}" style="display:none;"><i id="icon1" class="fa fa-shopping-cart" style="color:black;"></i><a id="text1" href="#">Add to bundle</a></p>
                    </div>
                    <div class="clearfix"></div>
                  </div>
                </div>
              </div> 
              {% for field in metafields %}
                {% assign value = field | last | split: ";" | first | split: ":" | last %}
                {% assign item = all_products[value] %}
                {% assign variants = item.variants %}
                {% assign variantSize = item.variants | size %}
              	{% if item.title != product.title %}
                  <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1 nopadding" style="width: 15px;">
                    <h2><span class="glyphicon glyphicon-plus"></span></h2>
                  </div>
                  <div class="col-xs-10 col-sm-4 col-md-1 col-lg-1 upsell-product">
                    <div class="col-item" id="item{{ forloop.index | plus: 1 }}" value="{{  }}">
                      <span class="tooltiptext">{{ item.title }}</span>
                      <div class="photo"><img alt="a" class="img-responsive upsell-image" src="{{ item | img_url: 'medium' }}"></div>
                      <div class="info">
                        <div class="row">
                          <div class="price col-md-12">
                            <h5>{{ item.title }}</h5>
                            <h5 id="variant-price{{ forloop.index | plus: 1 }}" class="price-text-color">{{ variants[0].price | money }}</h5>
                          </div>
                          <div class="variant col-md-12">
                          {% if variantSize <= 1 %}
                            <select id="variant{{ forloop.index | plus: 1 }}" class="variant-select" name="variant{{ forloop.index | plus: 1 }}" >
                              {% for variant in variants %}
                                <option value="{{ variant.id }}-{{ variant.price | money }}">{{ variant.title }}</option>
                              {% endfor %}
                            </select>
                          {% else %}
                            <select id="variant{{ forloop.index | plus: 1 }}" class="variant-select" name="variant{{ forloop.index | plus: 1 }}">
                              {% for variant in variants %}
                                <option value="{{ variant.id }}-{{ variant.price | money }}">{{ variant.title }}</option>
                              {% endfor %}
                            </select>
                          {% endif %}
                          </div>
                        </div>
                        <div class="rating col-md-12">
                          <i class="price-text-color fa fa-star"></i><i class="price-text-color fa fa-star"></i><i class="price-text-color fa fa-star"></i><i class="price-text-color fa fa-star"></i><i class="fa fa-star"></i>
                        </div>
                        <div class="separator">
                          <p id="btn-id{{ forloop.index | plus: 1 }}" class="btn-add"><input type="checkbox" id="c{{ forloop.index | plus: 1 }}" name="cb[]" value="{{ variants[0].id }}" style="display:none;"><i id="icon{{ forloop.index | plus: 1 }}" class="fa fa-shopping-cart" style="color:black;"></i><a id="text{{ forloop.index | plus: 1 }}" href="#">Add to bundle</a></p>
                        </div>
                        <div class="clearfix"></div>
                      </div>
                    </div>
                  </div>
              	{% endif %}
              {% endfor %}
              <div class="col-xs-1 col-sm-1 col-md1 col-lg-1 nopadding" style="width: 20px;">
                <div class="cart-confirmation">
                  <p style="font-size:17px; color:black; font-weight:bold;">
                    Total Price: <span id="total-price" style="color:red">$0.00</span>
                  </p>
                  <button class="btn upsell-button" id="add-upsell" name="add-upsell" type="submit" form="add-upsell-form">Add to Cart</button>
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
    {% endif %}
  {% endif %}
  <script type="text/javascript">
